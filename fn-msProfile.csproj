    <UserSecretsId>8a321741-b73e-48b6-9ef6-f27530265607</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration.UserSecrets" Version="6.0.1" />
